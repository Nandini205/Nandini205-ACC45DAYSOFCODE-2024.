#include <iostream>
using namespace std;

int main() {
    int num; 
    cin >> num;

    for (int i = 0; i < num; i++) {  
        int P, Q, R, S; 
        cin >> P >> Q >> R >> S;

      
        if (P > Q + R + S || Q > P + R + S || R > P + Q + S || S > P + Q + R) {
            cout << "YES" << endl;
        } else {
            cout << "NO" << endl;
        }
    }

    return 0;
}
