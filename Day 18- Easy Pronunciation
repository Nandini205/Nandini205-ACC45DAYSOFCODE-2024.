#include <iostream>
#include <string>
using namespace std;

int main() {
    int n;
cout<<"Enter the number";
    cin >> T;
    for (int n = 0; n < T; n++) {  
        int N;
        cin >> N;  
        string A;
        cin >> A; 
        
        int count0 = 0, count1 = 0;
        
      
        for (int i = 0; i < N; i++) {
            if (A[i] == '0') {
                count0++;
            } else {
                count1++;
            }
        }
        
       
        for (int i = 0; i < count0; i++) {
            cout << '0';
        }
        for (int i = 0; i < count1; i++) {
            cout << '1';
        }
        cout << endl;  
    }
    
    return 0;
}
