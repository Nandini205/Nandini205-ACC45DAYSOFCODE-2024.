#include <iostream>
using namespace std;

int main() {
    int n; 
cout<<"enter the number";
    cin >> n;
    
    while (n--) {
        int N; 
        cin >> N;
        
        int degree = -1; 
        for (int i = 0; i < N; ++i) {
            int Ai; 
            cin >> Ai;
            

            if (Ai != 0) {
                degree = i;
            }
        }
        
        cout << degree << endl;
    }
    
    return 0;
}
